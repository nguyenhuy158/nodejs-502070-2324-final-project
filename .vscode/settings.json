{
    "[handlebars]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
    },
    "breadcrumbs.enabled": false,
    "chat.editor.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "cSpell.diagnosticLevel": "Hint",
    "css.lint.emptyRules": "warning",
    "debug.console.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "diffEditor.codeLens": false,
    "diffEditor.experimental.showEmptyDecorations": false,
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingDelete": "always",
    "editor.autoClosingOvertype": "always",
    "editor.autoClosingQuotes": "always",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.codeActionWidget.showHeaders": true,
    "editor.codeLens": true,
    "editor.codeLensFontFamily": "JetBrains Mono, Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "editor.codeLensFontSize": 16,
    "editor.colorDecoratorsActivatedOn": "hover",
    "editor.columnSelection": false,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "block",
    "editor.cursorWidth": 0,
    "editor.detectIndentation": true,
    "editor.fastScrollSensitivity": 10,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.cursorMoveOnType": true,
    "editor.find.loop": true,
    "editor.foldingImportsByDefault": true,
    "editor.fontFamily": "JetBrains Mono, Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.fontVariations": true,
    "editor.fontWeight": "500",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.hover.enabled": false,
    "editor.hover.sticky": false,
    "editor.inlayHints.enabled": "off",
    "editor.inlayHints.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.insertSpaces": true,
    "editor.lightbulb.enabled": false,
    "editor.minimap.autohide": false,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 100,
    "editor.multiCursorModifier": "alt",
    "editor.occurrencesHighlight": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestionsDelay": 10,
    "editor.screenReaderAnnounceInlineSuggestion": true,
    "editor.scrollbar.horizontal": "auto",
    "editor.scrollbar.scrollByPage": false,
    "editor.scrollbar.vertical": "visible",
    "editor.selectionHighlight": false,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "editor.suggest.insertMode": "insert",
    "editor.suggest.preview": true,
    "editor.suggest.showStatusBar": false,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "editor.suggestSelection": "recentlyUsed",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.includeComments": "inUntrustedWorkspace",
    "editor.unicodeHighlight.nonBasicASCII": "inUntrustedWorkspace",
    "editor.wordWrap": "off",
    "files.autoSave": "onFocusChange",
    "files.simpleDialog.enable": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "html.autoClosingTags": true,
    "html.format.enable": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapAttributes": "auto",
    "javascript.format.semicolons": "insert",
    "javascript.preferences.quoteStyle": "single",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "liveServer.settings.AdvanceCustomBrowserCmdLine": "Chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "markdown.preview.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace, -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
    "material-icon-theme.folders.theme": "specific",
    "notebook.output.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "scm.inputFontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.fontFamily": "Cascadia Code, Fira Code, Hasklig, VictorMono, Monoid, Consolas, 'Courier New', monospace",
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.smoothScrolling": true,
    "window.zoomLevel": -1,
    "workbench.activityBar.iconClickBehavior": "toggle",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Tokyo Night Moon",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.list.smoothScrolling": true,
    "workbench.sideBar.location": "right",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 10,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.view.alwaysShowHeaderActions": false,
    "screencastMode.fontSize": 56,
    "markdown.preview.fontSize": 16,
    "zenMode.hideStatusBar": true,
    "zenMode.restore": true,
    "zenMode.silentNotifications": true,
    "zenMode.hideActivityBar": false,
    "window.autoDetectColorScheme": true,
    "workbench.settings.openDefaultKeybindings": false,
    "zenMode.hideLineNumbers": false,
    "workbench.settings.useSplitJSON": false,
    "terminal.integrated.fontSize": 12,
    "explorer.sortOrder": "type",
    "editor.inlineSuggest.showToolbar": "onHover",
    "editor.hover.delay": 10,
    "workbench.hover.delay": 10,
    "debug.console.wordWrap": false,
    "editor.tabCompletion": "on",
    "terminal.integrated.tabFocusMode": false,
    "editor.tabFocusMode": false,
    "emmet.triggerExpansionOnTab": true,
}